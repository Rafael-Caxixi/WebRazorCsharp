<MudCard>
    <MudCardMedia Image="images/cardCinema.png"></MudCardMedia>
    <MudCardHeader>
        <CardHeaderContent>
            <MudText Typo="Typo.h6">@Filme!.Nome</MudText>
        </CardHeaderContent>
    </MudCardHeader>
    <MudCardContent>
        <MudText Typo="Typo.body2">Cinema:@Filme!.Nome</MudText>
    </MudCardContent>
</MudCard>

@code {
    [Parameter]
    public FilmeResponse? Filme { get; set; }

}